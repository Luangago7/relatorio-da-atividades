Algoritmo "calcular a media da nota"

Var
nota1,nota2,media: real;


Inicio

   escreval("Digite a primeira nota: ")
   leia(nota1)

   escreval("Digite a segunda nota: ")
   leia(nota2)


   media := (nota1 + nota2) / 2


   escreval("A média do aluno é: ", media:0:2)


Fimalgoritmo